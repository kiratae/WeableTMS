<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Web</title>

    <environment include="Development">
        <!-- Custom fonts for this template-->
        <link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">

        <link href="~/vendor/sweetalert2/dist/sweetalert2.css" rel="stylesheet" type="text/css">
        <link href="~/vendor/pikaday/pikaday.css" rel="stylesheet" type="text/css">

    </environment>
    <environment exclude="Development">
        <!-- Custom fonts for this template-->
        <link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">

        <link href="~/vendor/sweetalert2/dist/sweetalert2.css" rel="stylesheet" type="text/css">
        <link href="~/vendor/pikaday/pikaday.css" rel="stylesheet" type="text/css">

    </environment>
    <!-- Custom styles for this template-->
    <link href="~/css/wefont.css" rel="stylesheet">
    <link href="~/css/site.css" rel="stylesheet">
    <link href="~/css/sb-admin-2.css" rel="stylesheet">

    <style>
        [data-toggle="collapse"] .arrow.fa:before {
            content: "\f077";
        }

        [aria-expanded="false"] .arrow.fa:before {
            content: "\f078";
        }

        [data-toggle="collapse"].collapsed .arrow.fa:before {
            content: "\f078";
        }
    </style>
</head>
<body id="page-top">
    <!-- Preloader -->
    <div class="preloader">
        <div class="spinner">
            <div class="double-bounce1"></div>
            <div class="double-bounce2"></div>
        </div>
    </div>
    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">
                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar static-top shadow">
                    <div class="navbar-nav mr-auto">
                        <!-- Sidebar - Brand -->
                        <a class="sidebar-brand d-flex align-items-center justify-content-center text-decoration-none" href="#">
                            <div class="sidebar-brand-icon">
                                <i class="fas fa-graduation-cap fa-3x"></i>
                            </div>
                            <div class="sidebar-brand-text mx-3"><h1 class="mt-2">TMS</h1></div>
                        </a>
                    </div>
                </nav>
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                @RenderBody()
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; Your Website 2019</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <environment include="Development">
        <!-- Bootstrap core JavaScript-->
        <script src="~/vendor/jquery/jquery.min.js"></script>
        <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

        <script src="~/vendor/moment/moment.js"></script>
        <script src="~/vendor/mask/masks.js"></script>
        <script src="~/vendor/sweetalert2/dist/sweetalert2.min.js"></script>
        <script src="~/vendor/pikaday/pikaday.js"></script>
        <script src="~/vendor/pikaday/pikaday.jquery.js"></script>

        <!-- Page level plugins -->
        <script src="~/vendor/chart.js/Chart.min.js"></script>
    </environment>
    <environment exclude="Development">
        <!-- Bootstrap core JavaScript-->
        <script src="~/vendor/jquery/jquery.min.js"></script>
        <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

        <script src="~/vendor/moment/moment.js"></script>
        <script src="~/vendor/mask/masks.js"></script>
        <script src="~/vendor/sweetalert2/dist/sweetalert2.min.js"></script>
        <script src="~/vendor/pikaday/pikaday.js"></script>
        <script src="~/vendor/pikaday/pikaday.jquery.js"></script>

        <!-- Page level plugins -->
        <script src="~/vendor/chart.js/Chart.min.js"></script>
    </environment>

    <!-- Custom scripts for all pages-->
    <script src="~/js/sb-admin-2.min.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/app.js" asp-append-version="true"></script>
    <script>
        $(document).ready(function () {
            $("form").attr('autocomplete', 'off');
        });
    </script>

    @RenderSection("Scripts", required: false)
</body>
</html>
